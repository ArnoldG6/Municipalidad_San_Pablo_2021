<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>PlanServlet</servlet-name>
        <servlet-class>plan.services.PlanServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanManager</servlet-name>
        <servlet-class>plan.services.PlanManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginManager</servlet-name>
        <servlet-class>plan.login.LoginManager</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PlanServlet</servlet-name>
        <url-pattern>/API/PlanServlet</url-pattern>
        <url-pattern>/API/PlanSearch</url-pattern>
        <url-pattern>/API/RetrievePlans</url-pattern>
        <url-pattern>/API/RetrievePlan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanManager</servlet-name>
        <url-pattern>/API/PlanManager/insert</url-pattern>
        <url-pattern>/API/PlanManager/edit</url-pattern>
        <url-pattern>/API/PlanManager/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginManager</servlet-name>
        <url-pattern>/API/LoginManager/test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
